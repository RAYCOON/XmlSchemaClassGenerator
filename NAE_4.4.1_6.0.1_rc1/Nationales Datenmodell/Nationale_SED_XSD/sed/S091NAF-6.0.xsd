	<xsd:element name="S091NAF" type="sedS091NAF:S091NAF">
		<xsd:annotation/>
	</xsd:element>
	<xsd:complexType name="AcceptedContestationsType">
		<xsd:sequence>
			<xsd:element name="AcceptedContestation" type="sedS091NAF:COCReferencesDebtorLiaisonBodyType" minOccurs="0" maxOccurs="unbounded">
