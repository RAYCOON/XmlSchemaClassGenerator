	<xsd:complexType name="ReplyCodeRPYCOCILPType">
		<xsd:annotation>
			
			<xsd:documentation xml:lang="de">
				<ccts:OfficialConstraintContextValue>
					<CodeListEntry>
						<value>01</value>
						<definition>Wir akzeptieren die Beanstandung</definition>
					</CodeListEntry>
					<CodeListEntry>
						<value>02</value>
						<definition>Wir lehnen ab. Weitere Informationen sind beigefügt</definition>
					</CodeListEntry>
					<CodeListEntry>
						<value>03</value>
						<definition>Korrigierte entsprechende Nummer der Zinsforderung ist</definition>
					</CodeListEntry>
					<CodeListEntry>
						<value>04</value>
						<definition>Korrigierter Betrag der Zinsgutschrift ist</definition>
					</CodeListEntry>
					<CodeListEntry>
						<value>99</value>
						<definition>sonstiger Grund [bitte Feld „sonstiger Grund“ ausfüllen]</definition>
					</CodeListEntry>
				</ccts:OfficialConstraintContextValue>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="value" minOccurs="1" maxOccurs="1">
				<xsd:annotation/>
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:pattern value="(01|02|03|04|99){1}"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
