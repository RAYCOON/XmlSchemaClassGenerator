{
  "name": "NAE Fixed Schema Conversion",
  "outputDirectory": "./output/NAE-Fixed",
  "globalSettings": {
    "namespacePrefix": "NAE",
    "generateNullables": true,
    "enableDataBinding": false,
    "generateInterfaces": false,
    "usePascalCase": true,
    "separateFiles": true,
    "separateNamespaceHierarchy": true,
    "collectionType": "System.Collections.Generic.List<>",
    "collectionSettersMode": "Public",
    "generateDescriptionAttribute": false,
    "uniqueTypeNames": true,
    "useShouldSerializePattern": false,
    "enumAsString": false,
    "enableUpaCheck": false,
    "doNotForceIsNullable": true,
    "compactTypeNames": false,
    "disableComments": true,
    "separateSubstitutes": true,
    "namespaceMappings": [
      {
        "xmlNamespace": "urn:eu:ec:dgempl:eessi:ns:model:common-types:v3",
        "cSharpNamespace": "NAE.Common.Types"
      },
      {
        "xmlNamespace": "urn:eu:ec:dgempl:eessi:ns:model:buc:v4",
        "cSharpNamespace": "NAE.BUC"
      },
      {
        "xmlNamespace": "urn:eu:ec:dgempl:eessi:ns:model:sed:v6",
        "cSharpNamespace": "NAE.SED"
      },
      {
        "xmlNamespace": "",
        "cSharpNamespace": "NAE.Common"
      }
    ]
  },
  "schemas": [
    {
      "name": "BasicTypes",
      "sourcePath": "schemas/NAE/BasicTypes-1.2.xsd",
      "outputSubDirectory": "Types",
      "enabled": true
    },
    {
      "name": "BusinessScope",
      "sourcePath": "schemas/NAE/BusinessScope-1.2.xsd",
      "outputSubDirectory": "Types",
      "enabled": true
    },
    {
      "name": "DocumentIdentification",
      "sourcePath": "schemas/NAE/DocumentIdentification-1.2.xsd",
      "outputSubDirectory": "Types",
      "enabled": true
    },
    {
      "name": "CC-4.4",
      "sourcePath": "schemas/NAE/CC-4.4.xsd",
      "outputSubDirectory": "CommonComponents",
      "settings": {
        "generateInterfaces": false,
        "separateSubstitutes": true,
        "doNotForceIsNullable": true
      },
      "enabled": true
    },
    {
      "name": "A100N-6.0",
      "sourcePath": "schemas/NAE/A100N-6.0.xsd",
      "outputSubDirectory": "A_Messages",
      "enabled": true
    },
    {
      "name": "ACK900N-6.0",
      "sourcePath": "schemas/NAE/ACK900N-6.0.xsd",
      "outputSubDirectory": "A_Messages",
      "enabled": true
    }
  ]
}