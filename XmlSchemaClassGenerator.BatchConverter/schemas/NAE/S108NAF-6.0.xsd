	<xsd:complexType name="ReplyMonthsInventoryCreditNoteContestationType">
		<xsd:annotation>
			<xsd:documentation xml:lang="de">
</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="replyCreditNotesContestationCode" type="sedS108NAF:ReplyCodeRPYCOCCRNIMOType" minOccurs="1" maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation xml:lang="de">
Antwortcode auf Beanstandung von Gutschriften
      </xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:choice>
				<xsd:element name="FillInConditionalIMONumber" type="sedS108NAF:ConditionalIMONumberType" minOccurs="0">
					<xsd:annotation>
						
						<xsd:documentation xml:lang="de">
Füllen Sie bitte folgendes Feld aus, wenn „Antwortcode auf Beanstandung von Gutschriften“ = „01 - Korrigierte entsprechende IMO-Nummer lautet“:
      </xsd:documentation>
					</xsd:annotation>
				</xsd:element>
				<xsd:element name="FillInConditionalNumberMonths" type="sedS108NAF:ConditionalNumberMonthsType" minOccurs="0">
					<xsd:annotation>
						
						<xsd:documentation xml:lang="de">
Füllen Sie bitte folgendes Feld aus, wenn „Antwortcode auf Beanstandung von Gutschriften“ = „02 - Korrigierte Monatsanzahl der Gutschrift ist“:
      </xsd:documentation>
					</xsd:annotation>
				</xsd:element>
				<xsd:element name="FillInConditionalCreditNoteContestation" type="sedS108NAF:ConditionalCreditNoteContestationType" minOccurs="0">
					<xsd:annotation>
						
						<xsd:documentation xml:lang="de">
Füllen Sie bitte folgendes Feld aus, wenn „Antwortcode auf Beanstandung von Gutschriften“ = „03 - Wir akzeptieren die Beanstandung“:
						</xsd:documentation>
					</xsd:annotation>
				</xsd:element>
				
				<xsd:element name="PleaseFillInIfReplyCreditNotesContestation" type="sedS108NAF:ReplyCreditNotesContestationCodeOtherType" minOccurs="0">
					<xsd:annotation>
						<xsd:documentation xml:lang="de">
Bitte ausfüllen, wenn "Antwortcode der Beanstandung der Gutschrift" = "99 - Sonstige [Feld 'Sonstige' ist auszufüllen]":
						</xsd:documentation>
					</xsd:annotation>
					</xsd:element>
				
			</xsd:choice>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="ReplyCodeRPYCOCCRNIMOType">
		<xsd:annotation>
			
			<xsd:documentation xml:lang="de">
				<ccts:OfficialConstraintContextValue>
					<CodeListEntry>
						<value>01</value>
						<definition>Korrigierte entsprechende IMO-Nummer lautet</definition>
					</CodeListEntry>
					<CodeListEntry>
						<value>02</value>
						<definition>Korrigierte Monatsanzahl der Gutschrift ist</definition>
					</CodeListEntry>
					<CodeListEntry>
						<value>03</value>
						<definition>Wir akzeptieren die Beanstandung</definition>
					</CodeListEntry>
					<CodeListEntry>
						<value>99</value>
						<definition>Sonstiges [bitte Feld „Sonstiges“ ausfüllen]</definition>
					</CodeListEntry>
				</ccts:OfficialConstraintContextValue>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="value" minOccurs="1" maxOccurs="1">
				<xsd:annotation/>
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:pattern value="(01|02|03|99){1}"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
